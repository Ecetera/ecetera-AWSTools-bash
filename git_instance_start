# Create the instance
ec2-run-instances ami-83cd5eb9 -n 1 -k EceteraTS --instance-type t1.micro -g sg-7538dd1a --region ap-southeast-2 --subnet subnet-72e0d21b
# Name i\the instance in the AWS EC2 console
ec2-create-tags i-35e43b09 -t Name=gitserver --region=ap-southeast-2
# associate Elastic IP (use ec2-describe-addresses --region ap-southeast-2 to find available IP addresses)
ec2-associate-address -i i-35e43b09 -a eipalloc-cb3e0ea2 --region ap-southeast-2

